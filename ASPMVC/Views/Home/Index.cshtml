    links.Add("Nous contacter", "/Home/Contact");*/

    Dictionary<string, string> links = new Dictionary<string, string>()
    {
        { "Accueil", "/" },
        { "S'enregistrer", "/Register" },
        { "Se connecter", "/Login" },
        { "Nous contacter", "/Home/Contact" },
    };
}
<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
</head>
<body>
    <header>
        <h1>Technofutur TIC</h1>
        <nav>
            <ul>
                @foreach(KeyValuePair<string, string> link in links)
                {
                    <li><a href="@link.Value">@link.Key</a></li>
                }
            </ul>
        </nav>
    </header>
    <main>
        <section>
            @if (TempData.ContainsKey("message"))
            {
                <div style="background-color:rgb(128,255,128); padding : 15px; border : 2px solid rgb(0,64,0); color : rgb(0,64,0); border-radius : 15px; text-align : center;">
                    <p>@TempData["message"]</p>
                </div>
            }
            <h1>Bienvenue sur Technofutur TIC</h1>
            <p>Lorem ipsum dolor sit, amet consectetur adipisicing elit. Excepturi repellat nam numquam ab reiciendis est alias quo similique enim voluptatibus, eius officiis molestias quis consectetur temporibus magni natus odit accusantium.</p>
        </section>
    </main>
    <footer>
        <p>Technofutur TIC - 2024/2025</p>
    </footer>
</body>
</html>